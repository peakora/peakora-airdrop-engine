# RPC and network
RPC_URLS=https://rpc.arbitrum.io,https://rpc.ankr.com/arbitrum,https://arb1.arbitrum.io/rpc
CHAIN_ID=42161

# Puppeteer chrome profile
CHROME_PATH=C:/Program Files/Google/Chrome/Application/chrome.exe   # <<< CHANGE THIS if different
CHROME_PROFILE_PATH=C:/Users/ibier/AppData/Local/Google/Chrome/User Data/Profile 1   # <<< CHANGE THIS to the profile you use for peakora

# Primary wallet (the MetaMask account you use to connect to sites)
PRIMARY_ADDRESS=0xYourPrimaryAddress   # <<< CHANGE THIS in your local .env only

# Private keys must NOT be committed. Keep real keys in a local .env only.
# In this example file we use placeholders.
PRIVATE_KEY_PRIMARY=your_primary_private_key_here   # <<< CHANGE THIS in your local .env only
PRIVATE_KEY_NODE=your_node_private_key_here         # <<< CHANGE THIS in your local .env only (optional)

# 1inch API
ONEINCH_API=https://api.1inch.io/v5.0

# Distribution settings
# Comma separated list of recipient addresses (the engine will send DISTRIBUTION_AMOUNT_ETH to each)
DISTRIBUTION_ADDRESSES=0xAddr1,0xAddr2,0xAddr3   # <<< CHANGE THIS to your 3 recipient addresses in local .env

DISTRIBUTION_THRESHOLD_ETH=0.02
DISTRIBUTION_AMOUNT_ETH=0.005

# Watcher
POLL_INTERVAL_MS=20000

# Quest automation
QUEST_URL=https://layer3.xyz/quests
QUEST_ACTION_SELECTOR=button[data-qa="start-quest"], .quest-card button

# Logging
LOG_LEVEL=info
