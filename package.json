    "dotenv": "^16.3.1",
    "axios": "^1.6.7",
    "ethers": "^6.11.1",
    "web3": "^4.2.0",
    "node-schedule": "^2.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  }
