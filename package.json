{
  "name": "peakora-airdrop-engine",
  "version": "1.0.0",
  "description": "Automation engine for collecting fuel and farming crypto airdrops with logging and dashboard monitoring.",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "gen-env-example": "node ./scripts/generate-env-example.js",

    "fuel": "node ./fuelCollector/fuelCollector.js",
    "galxe": "node ./fuelCollector/galxe.js",
    "zealy": "node ./fuelCollector/zealy.js",
    "quests": "node ./fuelCollector/quests.js",
    "test-fuel": "node ./fuelCollector/testFuel.js",
    "farm": "npm run fuel && npm run galxe && npm run zealy && npm run quests",

    "scheduler": "node ./scheduler/scheduler.js",
    "test-scheduler": "node ./scheduler/testScheduler.js",

    "tasks": "node ./scripts/tasks/task.js",
    "test-tasks": "node ./scripts/tasks/testTask.js",
    "airdrop": "npm run scheduler && npm run tasks",

    "logs": "node ./logger/viewLogs.js",
    "clear-logs": "rm -rf ./logs/*",

    "dashboard": "node ./dashboard/testDashboard.js",
    "central-station": "node ./centralStation/index.js"
  },
  "keywords": [
    "airdrop",
    "automation",
    "crypto",
    "layer2",
    "fuelCollector",
    "scheduler",
    "tasks",
    "logger",
    "dashboard",
    "monitoring",
    "peakora"
  ],
  "author": "Peakora Team",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "axios": "^1.6.7",
    "ethers": "^6.11.1",
    "web3": "^4.2.0",
    "node-schedule": "^2.1.1",
    "winston": "^3.10.0",
    "fs-extra": "^11.2.0",
    "express": "^4.19.2",
    "puppeteer": "^23.9.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  }
}
